 /* 幻灯开始 */
 * {margin:0;padding:0;}
ul, li, ol {list-style:none;}
img, a img {border:none;}
em, i, u {font-style:normal;}
a {text-decoration:none;color:#434343;}
a:hover {text-decoration:underline;color:#ff4000;}
.blank10 {clear:both;font-size:0;overflow:hidden;height:10px;}
.blank {clear:both;font-size:0;overflow:hidden;height:10px;background:#FFF;}
/*********content css定义***********/
h1, h2, h3 {font-family:"微软雅黑";color:#00659b;}
h1, h2, h3, h4, h5, h6 {height:24px;margin:0;font-weight:bold;}
/*********photo css定义***********/
.mb15 {margin-bottom: 0px;}
#photo_box {width:600px;margin:0;overflow:hidden;}
/*********图片切换 css定义***********/
.sub_box {width:600px;height:200px;float:left;position:relative;overflow:hidden;background:url() center center no-repeat;*background:url() 220px 120px no-repeat;}
.sub_box img {border:none;width:600px;height:200px;}
#bd1lfimg {position:relative;width:600px;height:200px;overflow:hidden;}
#bd1lfimg div {width:100000px;margin-left:-4px;}
#bd1lfimg dl {width:600px;height:200px;position:relative;overflow:hidden;float:left;}
#bd1lfimg dt {width:600px;height:200px;position:absolute;left:0px;top:0px;}
#bd1lfimg dd {width:600px;height:50px;background:#000;filter:alpha(Opacity=60);Opacity:0.7;position:absolute;left:0px;bottom:0px;padding:0 10px;}
#bd1lfimg dd h2 {height:20px;padding:8px 0 4px 0;line-height:10px;overflow:hidden;}
#bd1lfimg dd h2 a {font-size:14px;font-weight:bold;color:#fff;}
#bd1lfimg dd a {color:#fff;text-decoration:none;}
#bd1lfimg dd a:hover {text-decoration:none;color:#ccc;}
#bd1lfimg dd tt {color:#ddd;line-height:6px;}
#bd1lfimg dd tt a:hover {text-decoration:underline;}
.sub_nav {width:600px;height:25px;bottom:0px;position:absolute;color:#999;z-index:3;}
.sub_no {height:25px;float:right;}
.sub_no li {display:block;width:25px;height:25px;float:left;overflow:hidden;line-height:25px;vertical-align:middle;text-align:center;background:#000;filter:alpha(Opacity=50);Opacity:0.7;font-family:Arial;font-size:11px;color:#999;cursor:pointer;}
.sub_no li.show {background:#fff;color:#000;}
.sub_more {float:right;width:50px;height:25px;background:#000;filter:alpha(Opacity=50);Opacity:0.7;color:#999;text-align:center;line-height:25px;}
.sub_more a {color:#999;}
.sub_more a:hover {color:#ccc;text-decoration:none;}
 /* 幻灯结束 */

